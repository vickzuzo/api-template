{
  "name": "emedic-vendor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint .",
    "lint:ci": "yarn lint --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview",
    "format": "prettier --write src/*.{js,ts,tsx} src/**/*.{js,ts,tsx} src/**/**/*.{js,ts,tsx} src/**/**/**/*.{js,ts,tsx} src/**/**/**/**/*.{js,ts,tsx}",
    "format:ci": "prettier --check src/**/*.ts{,x}",
    "format-client": "prettier --write src/api/generated",
    "generate-client": "openapi --input https://server-testing-1234.com/api-docs-json --output ./src/api/generated --useOptions --useUnionTypes --request ./src/api/request.ts --client axios && yarn format-client"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.36.1",
    "@types/node": "^20.10.3",
    "axios": "^1.6.2",
    "clsx": "^2.0.0",
    "formik": "^2.4.5",
    "localforage": "^1.10.0",
    "lucide-react": "^0.294.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.3",
    "react-redux": "^9.0.2",
    "react-router-dom": "^6.20.1",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "sonner": "^1.2.4",
    "sort-by": "^1.2.0",
    "tailwind-merge": "^2.1.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/runtime": "^7.23.5",
    "@tanstack/eslint-plugin-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/redux-logger": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "enforce-branch-name": "^1.1.2",
    "eslint": "^8.53.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-ft-flow": "^3.0.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "openapi-typescript-codegen": "^0.25.0",
    "postcss": "^8.4.32",
    "prettier": "^3.1.0",
    "redux-devtools-extension": "^2.13.9",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-eslint": "^1.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "prettier --write"
  },
  "engines": {
    "node": ">=16"
  }
}
